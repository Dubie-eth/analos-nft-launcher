{
  "name": "analos-nft-launcher-backend",
  "version": "2.0.2",
  "description": "Backend for Analos NFT Launcher",
  "main": "dist/simple-server.js",
  "scripts": {
    "start": "node dist/simple-server.js",
    "build": "tsc && node -e \"require('fs').copyFileSync('src/analos-sdk-bridge.js', 'dist/analos-sdk-bridge.js')\" && node -e \"require('fs').copyFileSync('src/analos-sdk-runner.mjs', 'dist/analos-sdk-runner.mjs')\"",
    "dev": "ts-node src/simple-server.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "bs58": "^5.0.0",
    "axios": "^1.6.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}