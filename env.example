# Analos Environment Configuration
# Copy this to .env and fill in your values
# ⚠️ DO NOT COMMIT .env FILE!

# Network Configuration
SOLANA_NETWORK=devnet
RPC_URL=https://api.devnet.solana.com
# For mainnet: https://api.mainnet-beta.solana.com

# Key Paths (stored outside repository for security)
# Development
DEPLOYER_KEYPAIR_PATH=~/analos-keys/devnet/deployer.json
UPGRADE_AUTHORITY_KEYPAIR_PATH=~/analos-keys/devnet/deployer.json

# Production (mainnet) - Uncomment when ready
# MAINNET_DEPLOYER_KEYPAIR_PATH=~/analos-keys/mainnet/deployer.json
# MAINNET_UPGRADE_AUTHORITY_KEYPAIR_PATH=~/analos-keys/mainnet/upgrade-authority.json

# Program IDs (Safe to share publicly)
AIRDROP_PROGRAM_ID=
OTC_PROGRAM_ID=
VESTING_PROGRAM_ID=
TOKEN_LOCK_PROGRAM_ID=
METADATA_PROGRAM_ID=
MONITORING_PROGRAM_ID=
NFT_LAUNCHPAD_PROGRAM_ID=
PRICE_ORACLE_PROGRAM_ID=
RARITY_ORACLE_PROGRAM_ID=

# API Configuration
HELIUS_API_KEY=
QUICKNODE_API_KEY=

# Frontend Configuration
NEXT_PUBLIC_SOLANA_NETWORK=devnet
NEXT_PUBLIC_RPC_ENDPOINT=https://api.devnet.solana.com

